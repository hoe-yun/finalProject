<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.haribo.mapper.HomeMapper"></mapper>
	<select id="countEmployee" parameterType="com.example.haribo.vo.Employee" resultType="int">
		SELECT
			count(employee_no)
		FROM employee
		WHERE employee_active = 'Y'
	</select>
	
	<select id="countCustomer" parameterType="com.example.haribo.vo.Customer" resultType="int">
		SELECT
			count(customer_no)
		FROM customer
		WHERE customer_active = 'Y'
	</select>
	
	